{
  "name": "zorbio",
  "version": "1.1.0",
  "build": "1",
  "description": "Absorb the Orbs!",
  "main": "server/server.js",
  "scripts": {
    "test": "exit 0",
    "start": "./node_modules/.bin/supervisor --watch server,common,package.json server/server.js",
    "debug": "node-debug --web-port 8088 server/server.js",
    "start-prod": "node server/server.js dist",
    "css": "./node_modules/.bin/lessc client/css/main.less client/css/main.css && node_modules/postcss-cli/bin/postcss --use autoprefixer -r client/css/main.css",
    "build": "bash build.sh",
    "build-upload": "bash build.sh --upload",
    "uglify-theirs": "uglifyjs -o client/js/third.min.js",
    "uglify-ours": "uglifyjs -o client/js/first.min.js --wrap ZOR -m -c --mangle-props mangle-regex=\"/^z_|^ZOR/\""
  },
  "author": "Scripta Games, Inc.",
  "dependencies": {
    "animate.less": "2.2.0",
    "basic-auth": "1.1.0",
    "bufferutil": "3.0.1",
    "console-stamp": "0.2.5",
    "cookie": "0.3.1",
    "datasets": "0.1.0",
    "es6-promise": "4.1.0",
    "express": "4.15.3",
    "howler": "2.0.4",
    "ismobilejs": "0.4.1",
    "linode-near-location": "git+https://git@github.com/Jared-Sprague/linode-near-location.git",
    "lodash": "4.17.4",
    "node-gameloop": "0.1.4",
    "node-uuid": "1.4.8",
    "performance-now": "2.1.0",
    "profanity-censor": "1.0.5",
    "ractive": "0.9.1",
    "ractive-transitions-fade": "0.3.1",
    "raven": "2.1.0",
    "raven-js": "3.16.0",
    "request": "2.81.0",
    "schemapack": "1.4.2",
    "shader-particle-engine": "git+https://git@github.com/squarefeet/ShaderParticleEngine.git",
    "three": "0.86.0",
    "urijs": "1.18.10",
    "utf-8-validate": "3.0.2",
    "valid-url": "1.0.9",
    "web-audio-daw": "2.3.1",
    "whatwg-fetch": "2.0.3",
    "ws": "3.0.0",
    "xss-filters": "1.2.7"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "eslint": "4.1.1",
    "eslint-config-google": "0.8.0",
    "html-inline": "1.2.0",
    "html-minifier": "3.5.2",
    "less": "2.7.2",
    "node-inspector": "1.1.1",
    "postcss-cli": "4.1.0",
    "supervisor": "0.12.0",
    "uglify-js": "3.0.20"
  }
